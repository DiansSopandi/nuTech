{
    "version": 2,
    "builds": [
        {
            "src": "src/api/index.ts",
            "use": "@vercel/node",
            "config": { "includeFiles": ["src/**","dist/**","config/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api/index.ts"
        }
    ]
}